
.header-content {
    padding-bottom: 20px;
    font-family: "Merienda", cursive;
    font-optical-sizing: auto;
    font-weight: weight;
    font-style: normal;
    .main-btn{
        font-family: "Merienda", cursive;
        font-optical-sizing: auto;
        font-weight: weight;
        font-style: normal;
        color:#89c9d4;
    }

}
.header-content p{
    padding-bottom: 20px;
    margin: 1.5rem 0;
    line-height: 1.5;
    word-spacing: 3px;
   

}
.header-content{
    display: grid;
    grid-template-columns: repeat(1,1fr);
    align-items: center;
    justify-content: center;
    padding-left: 30%;
    padding-top: 10%;  
}

//resume download button---------------------

.cont-btn{
    display: flex;
    align-self:flex-start;

}
.main-btn
{
    border-radius: 30px;
    color:inherit;
    font-weight: 600;
    position: relative;
    border: 1px solid var(--color-white);
    display: flex;
    align-self: flex-start;
    display: flex;
    align-items: center;
    overflow: hidden;

    .btn-text{

        padding: 0 2rem;
        
    }

    .btn-icon{
            background-color: var(--color-secondary);
            display: flex;
            align-items: center;
            justify-content: center;
            border-radius: 50%;
            padding: 1rem;
    }
}
