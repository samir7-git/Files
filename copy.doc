       .timeline-item .python{
        
    border: 1px solid var (var(--color-grey-5));
    border-radius: 5px;
    box-shadow: 1px 2px 15px rgba(0, 0, 0, 0.3);
    transition: all 0.2s ease-in-out;
    line-height: 1.5;
     word-spacing: 3px;
     padding-top: 2.6rem;
     padding-bottom: 2.6rem;
     padding: 40px;
  
    h5{
      font-size: 1.3rem;
      color: #4bcae1;
      line-height: 1.8px;
      word-spacing: 3px;
      padding-bottom: 1rem;
  
    }
  
    span{
      font-size: 1rem;
      font-weight: 600;
    }
  
    h6{
      font-size: .8rem;
     font-weight: 200;
    }
    &:hover{
      padding: 2rem;
      border: 1px solid var(--color-grey-2);
      border-radius: 5px;
      box-shadow: 1px 2px 15px rgba(0, 0, 0, 0.3);
      transition: all 0.2s ease-in-out;
      }

   }